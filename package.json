{
  "name": "date-pikinator",
  "author": "Ryan L. Bell <ryan.l.bell@gmail.com>",
  "version": "0.1.0",
  "description": "Date picker React component",
  "main": "index.js",
  "keywords": ["date", "ui", "react"],
  "directories": ["stylesheets"],
  "repository": {
    "type": "git",
    "url": "https://github.com/kofno/date-pikinator"
  },
  "scripts": {
    "beefy": "http-server . -p 9000 & watchify demo.js -o demo-bundle.js --debug --verbose",
    "build-js": "browserify --standalone DatePikinator index.js > date-pikinator.js",
    "watch": "watchify index.js -o date-pikinator.js --debug --verbose",
    "watch-demo": "watchify demo.js -o demo-bundle.js --debug --verbose",
    "test": "browserify  test/*.js | testlingify"
  },
  "author": "Ryan L. Bell",
  "license": "MIT",
  "dependencies": {
    "browserify": "^5.9.1",
    "moment": "^2.8.1",
    "pikaday": "^1.2.0",
    "react": "^0.11.1"
  },
  "devDependencies": {
    "tape": "^2.13.4",
    "testling": "^1.7.0",
    "watchify": "^1.0.1"
  }
}
